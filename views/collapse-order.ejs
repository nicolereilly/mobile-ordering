  function getMenuHtml() {
	let menuHtml = "";
	menuArray.forEach((menu) => {

    let i = 1; //not wokring jjust yet
    
		menuHtml += `
      <div id="accordion">
      <div class="card">
        <div class="card-header">
          <a class="btn" data-bs-toggle="collapse" href="#collapse${i}">
            ${menu.category}
          </a>
        </div>`;

      menu.items.forEach((item) => { 


      menuHtml += `  

      <div id="collapse${i}" class="collapse show" data-bs-parent="#accordion">
        <div class="card-body">
          <div class="menu-items" id="menu-items">
            <span>${item.emoji}</span>
            <div class="details">
              <h3>${item.name} - <strong>$${item.price}</strong></div></h3>
              <button class="add-btn" data-item="${item.id}">Scoop</button>
               <button class="add-btn" data-item="${item.id}">Dish</button>
              <button class="add-btn" data-item="${item.id}">1/2 Pint</button>
              <button class="add-btn" data-item="${item.id}">Pint</button>
            </div>
          </div>
        </div>
      </div>
     

      `;
    })
    i++; 
	});
  console.log(menuHtml); 
	return menuHtml;
}


function renderMenu() {
	document.getElementById("menu").innerHTML = getMenuHtml();
